<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - lesagencesdepapa.fr</title>
    @Styles.Render("~/Content/theme")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/DevExtremeBundle")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/DevExtremeBundle")
    <link rel="stylesheet" href="~/Content/bootstrap-select.css" />
    @RenderSection("styles", false)

</head>
<body>
    @Html.Partial("_Header")
    <div class="page-content">
        @Html.Partial("_SideNavbar")
        <div class="content-wrapper">
            <div class="content">
                @RenderBody()
            </div>
            @Html.Partial("_Footer")
        </div>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/theme")
    <script src="~/Scripts/bootstrap-select.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
